<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:AbsolutePanel height="400px" width="500px"
			ui:field="absolutePanel">
			<g:at left="0" top="0">
				<g:AbsolutePanel height="400px" width="500px"
					ui:field="glassPanelContainer">
					<g:at left="0" top="0">
						<g:FocusPanel ui:field="focusPanel1">
							<g:SimplePanel/>
						</g:FocusPanel>
					</g:at>
				</g:AbsolutePanel>
			</g:at>
		</g:AbsolutePanel>
		<g:Button ui:field="resetButton">reset</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 