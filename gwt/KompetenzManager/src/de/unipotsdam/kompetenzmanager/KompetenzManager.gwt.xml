<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='kompetenzmanager'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.logging.Logging" />
	<set-property name="gwt.logging.logLevel" value="SEVERE" />
	<set-property name="gwt.logging.enabled" value="FALSE" />
	<set-property name="gwt.logging.consoleHandler" value="DISABLED" />
	<set-property name="gwt.logging.developmentModeHandler"
		value="DISABLED" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
	<set-property name="gwt.logging.systemHandler" value="DISABLED" />
	<set-property name="gwt.logging.firebugHandler" value="DISABLED" />
	<set-property name="gwt.logging.simpleRemoteHandler"
		value="ENABLED" />

	<!-- Other module inherits -->
	<!-- <inherits name='com.hydro4ge.raphaelgwt.RaphaelGWT' /> -->
	<!-- <inherits name='org.sgx.raphael4gwt.Raphael4Gwt'></inherits> -->

	<!-- Specify the app entry point class. -->
	<entry-point class='de.unipotsdam.kompetenzmanager.client.KompetenzManager' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
</module>
