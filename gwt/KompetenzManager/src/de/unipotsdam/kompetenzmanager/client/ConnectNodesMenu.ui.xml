<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		label {
			margin: 10px;
			font-family: fantasy;
			font-size: medium;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel height="72px">
			<g:Label text="choose other nodes.." styleName="chooseLabel"/>
			<g:Button width="122px" ui:field="finishedButton">finish</g:Button>
			<g:Button ui:field="button" width="121px">cancel</g:Button>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 