			</g:at>
			<g:at left="246" top="74">
				<g:AbsolutePanel width="624px" height="568px" ui:field="glassPanelContainer">
					<g:at left="0" top="0">
						<g:FocusPanel ui:field="focusPanel1" width="614px" height="541px"/>
