<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:SimplePanel ui:field="tabExplainationPanel"/>
		<g:VerticalPanel ui:field="competenceSelectionAndActivity">
			<g:HTMLPanel ui:field="competenceSelectionPanelPlaceholder"></g:HTMLPanel>
			<g:SimplePanel ui:field="HrPanelContainer" />
			<g:CaptionPanel captionText="Aktivitäten" styleName="requirementTextArea">
				<g:HTMLPanel ui:field="activityPlaceholder" />
			</g:CaptionPanel>														
			<g:SimplePanel ui:field="hrPanelContainer2" />
			<g:FocusPanel ui:field="warningPlaceholder" width="auto"
				height="auto" />
			<g:HorizontalPanel ui:field="buttonContainer">
				<b:Button icon="LINK" ui:field="submitButton">Markierte Kompetenzen und Aktivitäten verknüpfen</b:Button>				
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 