<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="deleteWidgetContainer">
		<g:SimplePanel ui:field="competenceSelectionHolder" />
		<b:Button icon="TRASH" ui:field="submitButton">Löschen</b:Button>
		<b:Alert type="SUCCESS" visible="false" ui:field="successAlert"
			close="false">Die
			Kompetenz(en) wurden gelöscht!</b:Alert>
		<b:Alert type="ERROR" animation="true" visible="false"
			ui:field="errorAlert" close="false">Es gab ein Problem beim Löschen der Kompetenzen, kontaktieren Sie einen Entwickler!</b:Alert>
	</g:HTMLPanel>
</ui:UiBinder> 