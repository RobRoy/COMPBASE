<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:TabPanel ui:field="tabPanel">
			<b:Tab heading="Kompetenzenverwaltung">
				<b:TabPanel>
					<b:Tab heading="Taxonomie">
						<b:TabPanel tabPosition="left">
							<b:Tab heading="Neue Kompetenz anlegen">
								<g:HTMLPanel ui:field="competenceCreationHolder" />
							</b:Tab>
							<b:Tab heading="Kompetenzen löschen">
								<g:HTMLPanel ui:field="competenceDeleteHolder" />
							</b:Tab>
							<b:Tab heading="Kompetenzhierarchie bearbeiten">
								<g:HTMLPanel ui:field="competenceHierarchieChangeHolder" />
							</b:Tab>
								<b:Tab heading="Einzelne Kompetenz bearbeiten">
								<g:HTMLPanel ui:field="competenceEditHolder" />
							</b:Tab>
							
							
						</b:TabPanel>
					</b:Tab>
					<b:Tab heading="Lernpfade">
						<g:HTMLPanel ui:field="pathTabPlaceholder" />
					</b:Tab>
				</b:TabPanel>
			</b:Tab>
			<b:Tab heading="Kurskontext">
				<b:TabPanel>
					<b:Tab heading="Kompetenzauswahl für Kurs">
						<g:HTMLPanel ui:field="requirementTabPlaceholder" />
					</b:Tab>

					<b:Tab heading="Zuordnung zu Aktivitäten">
						<g:HTMLPanel ui:field="linkTabPlaceholder" />
					</b:Tab>
					<b:Tab heading="Stand der Teilnehmer">
						<g:HTMLPanel ui:field="progressTabPlaceholder" />
					</b:Tab>
				</b:TabPanel>
			</b:Tab>
		</b:TabPanel>


